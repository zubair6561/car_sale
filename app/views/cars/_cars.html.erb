<div class="container">
  <table class="table table-bordered table-striped">
    <thead class="bg-dark text-white">
      <tr>
        <th>Car Model</th>
        <th>City</th>
        <th>Price</th>
        <th>Engine Details </th>
        <th>Transmission Type</th>
        <th>Color </th>
        <th>Milage (km/h)</th>
        <th>Engine Capacity (cc)</th>
        <th>Assembly Type</th>
      </tr>
    </thead>
    <tbody>
      <% @cars.each do |car| %>
        <% if !car&.post&.closed? %>
          <tr>
            <td><%= car.car_model %></td>
            <td><%= car.city %></td>
            <td><%= car.price %></td>
            <td><%= car.engine_detail %></td>
            <td><%= car.transmission_type %></td>
            <td><%= car.color%></td>
            <td><%= car.milage %></td>
            <td><%= car.engine_capacity %></td>
            <td><%= car.assembly_type %></td>  
          </tr>
        <% end %>
      <% end %>
    </tbody>
  </table> 
</div>
