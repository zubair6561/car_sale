<table class="table">
  <thead>
    <tr>
      <th scope="col">First Name</th>
      <th scope="col">Last Name</th>
      <th scope="col">Contact Number</th>
      <th scope="col">Email</th>
      
    </tr>
  </thead>
  <tbody>
    <%@users.each do |user| %>
      <tr>
        <td><%= user.firstname %></td>
        <td><%= user.lastname %></td>
        <td><%= user.contact_number %></td>
        <td><%= user.email %></td>
      </tr>
    <%end%>
  </tbody>
</table>
